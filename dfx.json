{
  "canisters": {
    "ecolegacy": {
      "type": "assets",
      "source": [
        "dist"
      ]
    },
    "eco_legacy": {
      "main": "src/main.rs",
      "type": "custom",
      "candid": "src/eco_legacy.did",
      "wasm": "eco_legacy.wasm"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": "@dfinity/webpack-plugin"
    }
  },
  "version": 1
}
